{
 "execution": [
  {
   "executor": "jmeter",
   "concurrency": 42,
   "ramp-up": "3s",
   "scenario": "api-test",
   "iterations": 1
  }
 ],
 "scenarios": {
  "api-test": {
   "requests": [
    {
     "url": "http://api:8000/v1/inventory",
     "method": "GET",
     "label": "get-inventory"
    },
    {
     "url": "http://api:8000/v1/movies",
     "method": "GET",
     "label": "get-movies"
    },
    {
     "url": "http://api:8000/v1/customers",
     "method": "GET",
     "label": "get-customers"
    }
   ]
  }
 },
 "reporting": [
  {
   "module": "console"
  },
  {
   "module": "final-stats",
   "summary": true,
   "percentiles": true,
   "failed-labels": false,
   "test-duration": true,
   "dump-csv": "./test/reports/perf_result_csv.csv"
  },
  {
   "module": "junit-xml",
   "data-source": "pass-fail",
   "filename": "./test/reports/junit-result.xml"
  },
  {
   "module": "blazemeter",
   "report-name": "API Performance Test 1",
   "test": "API Test v1",
   "project": "API Testing",
   "public-report": true,
   "token": "********",
   "filename": "./test/reports/junit-result.xml"
  }
 ],
 "settings": {
  "check-interval": "2s",
  "verbose": false
 }
}
