{
 "execution": [
  {
   "executor": "jmeter",
   "concurrency": 1,
   "ramp-up": "3s",
   "hold-for": "10s",
   "scenario": "api-test"
  }
 ],
 "scenarios": {
  "api-test": {
   "requests": [
    {
     "url": "http://api:8000/v2/inventory",
     "method": "GET",
     "label": "get-actors"
    },
    {
     "url": "http://api:8000/v2/movies",
     "method": "GET",
     "label": "get-movies"
    },
    {
     "url": "http://api:8000/v2/customers",
     "method": "GET",
     "label": "get-customers"
    }
   ]
  }
 },
 "reporting": [
  {
   "module": "console"
  },
  {
   "module": "final-stats",
   "summary": true,
   "percentiles": true,
   "failed-labels": false,
   "test-duration": true,
   "dump-csv": "TaurusResult/perf_result_csv.csv"
  },
  {
   "module": "junit-xml",
   "data-source": "pass-fail",
   "filename": "TaurusResult/junit-result.xml"
  }
 ],
 "settings": {
  "check-interval": "2s",
  "verbose": false
 }
}
